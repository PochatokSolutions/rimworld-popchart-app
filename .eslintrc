{
  "root": true,
  "env": {
    "es2020": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 11
  },
  "rules": {
    "no-var": "error",
    "indent": ["error", 2, {"SwitchCase": 1}],
    "no-multi-spaces": "error",
    "no-multiple-empty-lines": "error",
    "prefer-const": "error"
  }
}
